{
  "name": "wedapp",
  "version": "1.0.0",
  "description": "Wedding App",
  "main": "src/index.js",
  "scripts": {
    "build": "rm -r build/* && babel ./src --out-dir ./build && cp ./package.json ./.env ./Dockerfile ./.dockerignore ./build && cp -r ./src/views ./build && cp -r ./src/public ./build",
    "start": "npm run build && node ./build/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "lint": "eslint .",
    "test": "NODE_ENV=test mocha --require @babel/register --exit",
    "generate:token": "node -e 'console.log(require('crypto').randomBytes(256).toString('base64'));'",
    "knex": "babel-node node_modules/.bin/knex",
    "migrate": "npm run knex migrate:latest",
    "migrate:rollback": "npm run knex migrate:rollback --all",
    "seed": "npm run knex seed:run"
  },
  "author": "Evans Teiko",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.1",
    "knex": "^2.5.1",
    "mocha": "^10.2.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.5.2",
    "nodemailer": "^6.9.4",
    "objection": "^3.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/node": "^7.22.6",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/register": "^7.22.5",
    "@babel/runtime": "^7.22.6",
    "@faker-js/faker": "^8.0.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0"
  }
}
